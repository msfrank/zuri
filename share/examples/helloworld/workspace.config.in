{
    "zuri-build": {
        "global": {
            "sourceBaseUrl": "file://@SOURCE_DIRECTORY@",
            "packageDirectoryPathList": [ "@ZURI_BUILD_PACKAGES_DIR@" ]
        },
        "tasks": {
            "package:": {
                "packageDomain": "examples",
                "packageName": "helloworld",
                "majorVersion": @PROJECT_VERSION_MAJOR@,
                "minorVersion": @PROJECT_VERSION_MINOR@,
                "patchVersion": @PROJECT_VERSION_PATCH@,
                "mainLocation": "/helloworld"
            }
        }
    }
}
