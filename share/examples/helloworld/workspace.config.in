{
    "zuri": {
        "build": {
            "settings": {
                "global": {
                    "sourceBasePath": "src"
                }
            }
        },
        "targets": {
            "std": {
                "type": "Package",
                "packageUrl": "file://@ZURI_STD_FILEPATH@"
            },
            "helloworld": {
                "type": "Program",
                "specifier": "helloworld-@PROJECT_VERSION@@examples",
                "programMain": "/helloworld",
                "programModules": [],
                "depends": ["std"]
            }
        }
    }
}
